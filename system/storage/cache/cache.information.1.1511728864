u0436\u0434\u0430\u0435\u0442\u0441\u044f\r\n\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c \u0443\u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u0435\u043d\u0438\u0435\u043c, \u0441\u0435\u0440\u0442\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u043c \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u044f, \u0434\u0435\u043a\u043b\u0430\u0440\u0430\u0446\u0438\u0435\u0439 \u043e\r\n\u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438. \u041f\u043e\u0441\u0442\u0430\u0432\u0449\u0438\u043a \u0433\u0430\u0440\u0430\u043d\u0442\u0438\u0440\u0443\u0435\u0442 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435 \u043a\u043e\u043d\u0434\u0438\u0442\u0435\u0440\u0441\u043a\u0438\u0445 \u0438\u0437\u0434\u0435\u043b\u0438\u0439\r\n\u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u043c \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f\u043c \u043f\u0440\u0438 \u0441\u043e\u0431\u043b\u044e\u0434\u0435\u043d\u0438\u0438 \u0443\u0441\u043b\u043e\u0432\u0438\u0439 \u0442\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u0438\u0440\u043e\u0432\u043a\u0438 \u0438 \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f\r\n\u0414\u0430\u043d\u043d\u044b\u0435 \u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u0438, \u2116 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0443\u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u0435\u043d\u0438\u044f, \u0441\u0440\u043e\u043a\u0438 \u0440\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438 \u0438\r\n\u0434\u0440\u0443\u0433\u0438\u0435 \u0434\u0430\u043d\u043d\u044b\u0435 \u0443\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0442\u0441\u044f \u0432 \u0442\u043e\u0432\u0430\u0440\u043d\u043e-\u0442\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u043d\u043e\u0439 \u043d\u0430\u043a\u043b\u0430\u0434\u043d\u043e\u0439, \u043a\u043e\u0442\u043e\u0440\u0430\u044f\r\n\u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u0435\u0442 \u043a\u0430\u0436\u0434\u0443\u044e \u043f\u0430\u0440\u0442\u0438\u044e \u0442\u043e\u0432\u0430\u0440\u0430. &lt;o:p&gt;&lt;\/o:p&gt;&lt;\/span&gt;&lt;\/p&gt;\r\n\r\n&lt;p class=&quot;MsoNormal&quot; style=&quot;color: rgb(102, 102, 102); font-family: &amp;quot;Open Sans&amp;quot;, sans-serif; text-align: justify;&quot;&gt;&lt;span style=&quot;mso-bidi-font-size:\r\n9.0pt;font-family:&amp;quot;Arial&amp;quot;,sans-serif;background:yellow;mso-highlight:yellow&quot;&gt;3.2&lt;\/span&gt;&lt;span style=&quot;mso-bidi-font-size:9.0pt;font-family:&amp;quot;Arial&amp;quot;,sans-serif&quot;&gt;. \u041a\u043e\u043d\u0434\u0438\u0442\u0435\u0440\u0441\u043a\u0438\u0435\r\n\u0438\u0437\u0434\u0435\u043b\u0438\u044f \u0434\u043e\u043b\u0436\u043d\u044b \u0445\u0440\u0430\u043d\u0438\u0442\u044c\u0441\u044f \u0432 \u0447\u0438\u0441\u0442\u044b\u0445, \u0445\u043e\u0440\u043e\u0448\u043e \u0432\u0435\u043d\u0442\u0438\u043b\u0438\u0440\u0443\u0435\u043c\u044b\u0445 \u043f\u043e\u043c\u0435\u0449\u0435\u043d\u0438\u044f\u0445, \u043d\u0435\r\n\u0437\u0430\u0440\u0430\u0436\u0435\u043d\u043d\u044b\u0445 \u0432\u0440\u0435\u0434\u0438\u0442\u0435\u043b\u044f\u043c\u0438 \u0445\u043b\u0435\u0431\u043d\u044b\u0445 \u0437\u0430\u043f\u0430\u0441\u043e\u0432 \u0438 \u043f\u0440\u043e\u0447\u0438\u043c\u0438 \u0432\u0440\u0435\u0434\u0438\u0442\u0435\u043b\u044f\u043c\u0438 \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435\r\n(18+3) \u0438 \u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0432\u043b\u0430\u0436\u043d\u043e\u0441\u0442\u0438 \u0432\u043e\u0437\u0434\u0443\u0445\u0430 \u043d\u0435 \u0431\u043e\u043b\u0435\u0435 75 % \u0438 \u043d\u0435 \u0434\u043e\u043b\u0436\u043d\u044b \u043f\u043e\u0434\u0432\u0435\u0440\u0433\u0430\u0442\u044c\u0441\u044f\r\n\u0432\u043e\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044e \